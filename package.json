{
  "name": "nilbog",
  "version": "0.1.0",
  "description": "MutationObserver shortcuts to prevent certain actions on nodes.",
  "main": "dist/index.js",
  "repository": "https://github.com/czycha/nilbog",
  "author": "czycha",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "gulp js:prod",
    "watch": "gulp",
    "lint": "gulp lint",
    "clean": "rimraf dist/ docs/",
    "docs": "rimraf docs/ && npm run lint && jsdoc -d docs -r src/ -R README.md --verbose",
    "test": "jest",
    "prepack": "npm run clean && npm run build",
    "serve": "node test/server.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cli": "^2.0.1",
    "gulp-eslint": "^4.0.2",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-puppeteer": "^2.4.0",
    "jsdoc": "^3.5.5",
    "puppeteer": "^1.3.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-stream": "^4.0.3"
  },
  "dependencies": {
    "uid": "^0.0.2"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "globals": {
      "PATHS": {
        "index": "http://localhost:4444",
        "blank": "http://localhost:4444/blank.html"
      }
    },
    "setupFiles": ["./test/setup.js"],
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  }
}
